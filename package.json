{
  "name": "naklikam",
  "version": "1.0.0",
  "description": "Open-source AI website builder. Describe your website, AI builds it. Alternative to Lovable, Bolt.new, v0.dev.",
  "author": "Tadeas Raska <tadeas@raska.eu>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "node server.js",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 50",
    "preview": "vite preview",
    "dev:sandbox": "npx serve sandbox -p 3003 --cors"
  },
